{
  "name": "astral-draft",
  "version": "1.0.0",
  "type": "module",
  "description": "Elite Fantasy Football",
  "main": "index.tsx",
  "scripts": {
    "dev": "vite --port 8765",
    "dev:oracle": "vite --port 8765 --open",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "start": "vite",
    "server": "npx ts-node --project tsconfig.backend.json backend/server.ts",
    "server:dev": "npx nodemon --exec \"npx ts-node --project tsconfig.backend.json backend/server.ts\"",
    "server:prod": "NODE_ENV=production npx ts-node --project tsconfig.backend.json backend/server.ts",
    "server:db": "npx ts-node --project tsconfig.backend.json backend/server-with-db.ts",
    "server:db:dev": "npx nodemon --exec \"npx ts-node --project tsconfig.backend.json backend/server-with-db.ts\"",
    "oracle:server": "npx ts-node --project tsconfig.backend.json backend/oracleServer.ts",
    "oracle:dev": "npx nodemon --exec \"npx ts-node --project tsconfig.backend.json backend/oracleServer.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy": "npm run build:prod && netlify deploy --prod"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@google/genai": "^1.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/validator": "^13.15.2",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.23.3",
    "helmet": "^7.2.0",
    "idb": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.535.0",
    "nodemailer": "^7.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.3",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "stream-browserify": "^3.0.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "util": "^0.12.5",
    "vite": "^7.0.6"
  }
}
